function getTime(){
    var d = new Date();
    var hours = d.getHours().toString();
    var minutes = d.getMinutes().toString();
    var timeFlag = "AM";
    if(parseInt(hours) > 12){
        hours = hours - 12;
        timeFlag = "PM";
    }
    if(hours.length==1){
        hours = "0"+hours;
    }
    if(minutes.length==1){
        minutes = "0"+minutes;
    }

    var time = hours+":"+minutes+" "+timeFlag;
    return time;
}

 

